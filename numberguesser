import random
guess = 0
score = 100
target = random.randint(1,10)


print ("Your score is ", 100)
print ("Guess a number between 1 and 10")

while score > 0:
    guess = int(input())
   
    if guess == target:  
       print ("Correct! You won!")
       break
    if guess > 10:
       print ("That's not between 1 and 10! Guess again")
    else:
       score = score - random.randint(10,40)
       print ("Guess again! Your remaining score is ", score)

            

    




